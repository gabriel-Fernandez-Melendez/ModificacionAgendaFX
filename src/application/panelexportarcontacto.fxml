<!--  POR ALGUNA RAZON SUPERIOR A MI ENTENDIMIENTO NO ES NECESARIO PONER AQUI EL CONTROLADOR -->

<fx:root prefHeight="600.0" prefWidth="650.0" type="AnchorPane" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <Pane prefHeight="600.0" prefWidth="650.0">
         <children>
            <VBox layoutX="-5.0" layoutY="-4.0" prefHeight="600.0" prefWidth="658.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Exportar Contacto">
                           <font>
                              <Font size="51.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="120.0" top="30.0" />
                           </HBox.margin>
                        </Label>
                     </children></HBox>
                  <HBox layoutX="10.0" layoutY="10.0" prefHeight="458.0" prefWidth="658.0">
                     <children>
                        <ListView fx:id="lista" prefHeight="200.0" prefWidth="200.0">
                           <HBox.margin>
                              <Insets bottom="20.0" left="30.0" top="20.0" />
                           </HBox.margin>
                        </ListView>
                        <VBox prefHeight="458.0" prefWidth="374.0">
                           <children>
                              <Label prefHeight="44.0" prefWidth="197.0" text="haga click en el contacto a exportar">
                                 <VBox.margin>
                                    <Insets left="110.0" top="80.0" />
                                 </VBox.margin>
                              </Label>
                              <Label layoutX="130.0" layoutY="90.0" prefHeight="44.0" prefWidth="197.0" text="luego al boton Exportar Contacto">
                                 <VBox.margin>
                                    <Insets left="110.0" />
                                 </VBox.margin>
                              </Label>
                              <Button fx:id="exportar" mnemonicParsing="false" onAction="#ExportarContacto" text="Exportar Contacto">
                                 <VBox.margin>
                                    <Insets left="150.0" top="40.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children></HBox>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</fx:root>
